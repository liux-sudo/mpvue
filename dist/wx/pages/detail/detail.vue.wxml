      <view class="_div data-v-92bc3422 line"></view>
    </view> <button class="_button data-v-92bc3422 btn" open-type="share">转发此文章</button>
    <view class="_p data-v-92bc3422 detail_footer_content">{{detailsList.detail_content}}</view>
  </view>
</template>