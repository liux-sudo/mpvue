<template name="69019b92">
  <view class="_div data-v-92bc3422 detail">
    <view class="_div data-v-92bc3422 detail_img">
      <image class="_img data-v-92bc3422 detail_img_bg" src="{{detailsList.detail_img}}" alt></image>
      <image bindtap="handleProxy" class="_img data-v-92bc3422 music" src="{{isMusic ? '/static/images/music-start.png' : '/static/images/music-stop.png' }}" alt data-eventid="{{'0'}}" data-comkey="{{$k}}"></image>
    </view>
    <view class="_div data-v-92bc3422 detail_from">
      <image src="{{detailsList.avatar}}" alt class="_img data-v-92bc3422"></image> <label class="_span data-v-92bc3422">{{detailsList.date}}</label>
